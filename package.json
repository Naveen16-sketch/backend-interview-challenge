{
  "name": "backend-interview-challenge",
  "version": "1.0.0",
  "description": "Backend interview challenge project",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --watch src --exec tsx src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Naveen16-sketch/backend-interview-challenge.git"
  },
  "keywords": [],
  "author": "Naveen Bhajantri",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Naveen16-sketch/backend-interview-challenge/issues"
  },
  "homepage": "https://github.com/Naveen16-sketch/backend-interview-challenge#readme",
  "dependencies": {
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "sqlite3": "^5.1.7",
    "uuid": "^11.1.0",

    "@types/node": "^20.14.10",
    "@types/express": "^5.0.3",
    "@types/cors": "^2.8.19",
    "@types/uuid": "^10.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.34.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "tsx": "^4.20.5",
    "typescript": "^5.6.3",
    "vitest": "^3.2.4"
  }
}
